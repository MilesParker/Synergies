      <members xsi:type="act:AEvaluate" label="Increment Guide ID" ID="incrementGuideID" sources="//@agents[ID='guide']/@rootActivity/@members[ID='setGuideIDToNextGuideID']" targets="//@agents[ID='guide']/@rootActivity/@members[ID='setNextGuideIDToIncrementGuideID']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='initializeID']">
        <inputs selected="//@agents[ID='guide']/@rootActivity/@members[ID='initializeID']" value="//@agents[ID='guide']/@attributes[ID='guideID']"/>
        <function xsi:type="function:FOperator" href="http://metaabm.org/core_library.metaabm#//@library[ID='coreFunctions']/@subs[ID='operators']/@subs[ID='numericOperators']/@functions[ID='increment']"/>
