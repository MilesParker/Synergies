  <agents label="Learner" ID="learner" pluralLabel="Learners">
    <attributes label="Learning Interest" ID="learningInterest" pluralLabel="Learning Interests" description="How engaged is the learner but the process of learning itelf?" sType="REAL" gatherData="true"/>
    <attributes label="Learner ID" ID="learnerID" pluralLabel="Learner IDs" description="A unique identifier for the learner."/>
    <attributes label="Learner Index" ID="learnerIndex" pluralLabel="Learner Indexs" description="(Internal Use.)" sType="REAL" units="" derived="true"/>
    <attributes label="Guide ID" ID="guideID" pluralLabel="Guide IDs" description="A target resource that the learner may be seeking. Once a learner has identified a resource, the learner will seek this same resource out in the future unless they happen upon another resource. This id could be 0 in which case the learner hasn't identified a target learning resource."/>
    <attributes label="Self Contribution" ID="selfContribution" pluralLabel="Self Contributions" description="To what extent has the learner contributed to the growth of their own learning interest, excluding the impact of guide influence?" sType="REAL" gatherData="true"/>
    <rootActivity xsi:type="act:AGroup" label="Actions" ID="actions" pluralLabel="Actionss">
      <members xsi:type="act:AInitialize" label="Initialize State" ID="initializeState" targets="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest'] //@agents[ID='learner']/@rootActivity/@members[ID='setLearnerIDToNextLearnerID']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" agent="//@agents[ID='learner']" pluralLabel="Initialize States"/>
      <members xsi:type="act:AEvaluate" label="Initial Interest" ID="initialInterest" sources="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" targets="//@agents[ID='learner']/@rootActivity/@members[ID='setLearningInterestToInitialInterest']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']">
        <inputs selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" value="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest']/@inputs.0/@literal">
          <literal xsi:type="act:AMultiValue" label="Initial Interest States" ID="initialInterestStates"/>
        </inputs>
        <inputs selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" value="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest']/@inputs.1/@literal">
          <literal xsi:type="act:AMultiValue" label="Initial Interest Minimum" ID="initialInterestMinimum" values="//@attributes[ID='initialInterestMinimum']"/>
        </inputs>
        <inputs selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" value="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest']/@inputs.2/@literal">
          <literal xsi:type="act:AMultiValue" label="Initial Interest Maximum" ID="initialInterestMaximum" values="//@attributes[ID='initialInterestMaximum']"/>
        </inputs>
        <function xsi:type="function:FGenericFunction" href="http://metaabm.org/core_library.metaabm#//@library[ID='coreFunctions']/@subs[ID='distribution']/@functions[ID='uniformAdditiveDistribution']"/>
      </members>
      <members xsi:type="act:ASet" label="Set Learning Interest To Initial Interest" ID="setLearningInterestToInitialInterest" sources="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" attribute="//@agents[ID='learner']/@attributes[ID='learningInterest']" parameter="//@agents[ID='learner']/@rootActivity/@members[ID='initialInterest']"/>
      <members xsi:type="act:ASelect" label="New Guide" ID="newGuide" sources="//@agents[ID='learner']/@rootActivity/@members[ID='pickGuide']" targets="//@agents[ID='learner']/@rootActivity/@members[ID='connectLearner'] //@agents[ID='learner']/@rootActivity/@members[ID='guideIDAddZero'] //@agents[ID='learner']/@rootActivity/@members[ID='incrementLearnerCount']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='pickGuide']" agent="//@agents[ID='guide']"/>
      <members xsi:type="act:ARule" label="Guided Growth" ID="guidedGrowth" targets="//@agents[ID='learner']/@rootActivity/@members[ID='guide']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='guidedGrowth']" agent="//@agents[ID='learner']" pluralLabel="Guided Growths" description="Defines the effect guides might have on a learner's growth in learning interest. Every period, the guide efficacy is compared to the learners interest. If the learner's interest is already at or above the guide's efficacy, then there is no effect. Otherwise, we look at the gap between the guide's efficacy and the learner's interest. The greater the gap, the more potential the guide has to affect learning interest. This value is then factored down by the guide contribution rate.&#xA;"/>
      <members xsi:type="act:ASelect" label="Guide" ID="guide" sources="//@agents[ID='learner']/@rootActivity/@members[ID='guidedGrowth']" targets="//@agents[ID='learner']/@rootActivity/@members[ID='neighborQuery']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='guidedGrowth']" agent="//@agents[ID='guide']" space="//@projections[ID='guideRelationships']"/>
      <members xsi:type="act:ASet" label="Set Learner ID To Next Learner ID" ID="setLearnerIDToNextLearnerID" sources="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" targets="//@agents[ID='learner']/@rootActivity/@members[ID='incrementNextLearnerID']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" attribute="//@agents[ID='learner']/@attributes[ID='learnerID']" parameter="//@attributes[ID='nextLearnerID']"/>
      <members xsi:type="act:AEvaluate" label="Increment Next Learner ID" ID="incrementNextLearnerID" sources="//@agents[ID='learner']/@rootActivity/@members[ID='setLearnerIDToNextLearnerID']" targets="//@agents[ID='learner']/@rootActivity/@members[ID='setNextLearnerIDToIncrementNextLearnerID']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']">
        <inputs selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" value="//@attributes[ID='nextLearnerID']"/>
        <function xsi:type="function:FOperator" href="http://metaabm.org/core_library.metaabm#//@library[ID='coreFunctions']/@subs[ID='operators']/@subs[ID='numericOperators']/@functions[ID='increment']"/>
      </members>
      <members xsi:type="act:ASet" label="Set Next Learner ID To Increment Next Learner ID" ID="setNextLearnerIDToIncrementNextLearnerID" sources="//@agents[ID='learner']/@rootActivity/@members[ID='incrementNextLearnerID']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='initializeState']" attribute="//@attributes[ID='nextLearnerID']" parameter="//@agents[ID='learner']/@rootActivity/@members[ID='incrementNextLearnerID']"/>
      <members xsi:type="act:ADerive" label="Derive Learner Index" ID="deriveLearnerIndex" targets="//@agents[ID='learner']/@rootActivity/@members[ID='identityLearnerID']" selected="//@agents[ID='learner']/@rootActivity/@members[ID='deriveLearnerIndex']" attribute="//@agents[ID='learner']/@attributes[ID='learnerIndex']" agent="//@agents[ID='learner']"/>
