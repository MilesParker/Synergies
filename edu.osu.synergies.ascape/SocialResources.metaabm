          <inputs selected="//@agents[ID='guide']/@styles[ID='guideStyle2D']/@rootActivity/@members[ID='defaultSimpleResourcesAgentStyle']" value="//@agents[ID='guide']/@styles[ID='guideStyle2D']/@rootActivity/@members[ID='defaultSimpleResourcesAgentColor']/@inputs.2/@literal">
