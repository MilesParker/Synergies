      </members>
      <members xsi:type="act:ASet" label="Set Efficacy To Guide Efficacy" ID="setEfficacyToGuideEfficacy" sources="//@agents[ID='guide']/@rootActivity/@members[ID='guideEfficacy']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='initializeState']" attribute="//@agents[ID='guide']/@attributes[ID='efficacy']" parameter="//@agents[ID='guide']/@rootActivity/@members[ID='guideEfficacy']"/>
      <members xsi:type="act:AInitialize" label="Initialize ID" ID="initializeID" targets="//@agents[ID='guide']/@rootActivity/@members[ID='setGuideIDToNextGuideID']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='initializeID']" agent="//@agents[ID='guide']" pluralLabel="Initialize IDs"/>
