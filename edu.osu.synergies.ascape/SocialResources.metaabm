      <members xsi:type="act:ASet" label="Set Next Guide ID To Increment Guide ID" ID="setNextGuideIDToIncrementGuideID" sources="//@agents[ID='guide']/@rootActivity/@members[ID='incrementGuideID']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='initializeID']" attribute="//@attributes[ID='nextGuideID']" parameter="//@agents[ID='guide']/@rootActivity/@members[ID='incrementGuideID']"/>
      <members xsi:type="act:ADerive" label="Derive Contribution Per Learner" ID="deriveContributionPerLearner" targets="//@agents[ID='guide']/@rootActivity/@members[ID='learnerCountGreaterZeroQuery'] //@agents[ID='guide']/@rootActivity/@members[ID='learnerCountIdenticalZeroQuery']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']" attribute="//@agents[ID='guide']/@attributes[ID='contributionPerLearner']" agent="//@agents[ID='guide']" space="//@projections[ID='guideRelationships']"/>
      <members xsi:type="act:AEvaluate" label="Contribution Total Divide Learner Count" ID="contributionTotalDivideLearnerCount" sources="//@agents[ID='guide']/@rootActivity/@members[ID='learnerCountGreaterZeroQuery']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']">
