    <options label="Random" ID="random" pluralLabel="Randoms"/>
  </attributes>
  <attributes label="RegularYCount" ID="regularYCount" pluralLabel="RegularYCounts" description="When distributing regularly, the number of rows of resources to create. That is, how many horizontal lines will we ditribute all resources across?" defaultValue="2"/>
  <attributes label="BiasedRegionX" ID="biasedRegionX" pluralLabel="BiasedRegionXs" sType="REAL" defaultValue=".5"/>
  <attributes label="BiasedRegionY" ID="biasedRegionY" pluralLabel="BiasedRegionYs" sType="REAL" defaultValue=".5"/>
