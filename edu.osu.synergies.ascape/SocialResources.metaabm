        <inputs selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']" value="//@attributes[ID='learnerCount']"/>
        <inputs selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']">
          <value xsi:type="metaabm:SAttribute" href="http://metaabm.org/core_library.metaabm#//@attributes[ID='zero']"/>
        </inputs>
        <function xsi:type="function:FLogicalOperator" href="http://metaabm.org/core_library.metaabm#//@library[ID='coreFunctions']/@subs[ID='operators']/@subs[ID='logicalOperators']/@functions[ID='greater']"/>
      </members>
      <members xsi:type="act:AQuery" label="Learner Count Identical Zero Query" ID="learnerCountIdenticalZeroQuery" sources="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']" targets="//@agents[ID='guide']/@rootActivity/@members[ID='originalValueZero']" selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']">
        <inputs selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']" value="//@agents[ID='guide']/@attributes[ID='learnerCount']"/>
        <inputs selected="//@agents[ID='guide']/@rootActivity/@members[ID='deriveContributionPerLearner']">
